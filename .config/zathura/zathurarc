set recolor true
set default-bg '#151523'
set default-fg '#a8afa5'
set recolor-lightcolor '#151523'
set recolor-darkcolor '#a8afa5'
set statusbar-bg '#151523'
set statusbar-fg '#a8afa5'
set inputbar-bg '#151523'
set inputbar-fg '#a8afa5'
set font 'Fantasque Sans Mono normal 12'
map <A-\<> goto top
map <A-\>> goto bottom
map <C-n> scroll down
map <C-p> scroll up

